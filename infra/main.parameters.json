{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "name": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "principalId": {
        "value": "${AZURE_PRINCIPAL_ID}"
      },
      "openAILocation": {
        "value": "${AZURE_OPENAI_LOCATION}"
      },
      "openAiModelName": {
        "value": "${AZURE_OPENAI_MODEL=gpt-4o}"
      },
      "openAiModelVersion": {
        "value": "${AZURE_OPENAI_MODEL_VERSION=2024-05-13}"
      },
      "openAiDeploymentName": {
        "value": "${AZURE_OPENAI_DEPLOYMENT=gpt-4o}"
      },
      "openAiDeploymentCapacity": {
        "value": "${AZURE_OPENAI_DEPLOYMENT_CAPACITY=30}"
      },
      "openAiDeploymentSkuName": {
        "value": "${AZURE_OPENAI_DEPLOYMENT_SKU_NAME=GlobalStandard}"
      },
      "openAiResourceName": {
        "value": "${AZURE_OPENAI_RESOURCE}"
      },
      "openAiResourceGroupName": {
        "value": "${AZURE_OPENAI_RESOURCE_GROUP}"
      },
      "openAiResourceGroupLocation": {
        "value": "${AZURE_OPENAI_RESOURCE_GROUP_LOCATION}"
      },
      "openAiSkuName": {
        "value": "${AZURE_OPENAI_SKU_NAME=S0}"
      },
      "openAiApiVersion": {
        "value": "${AZURE_OPENAI_API_VERSION}"
      },
      "createAzureOpenAi": {
        "value": "${CREATE_AZURE_OPENAI=true}"
      },
      "openAiKey": {
        "value": "${AZURE_OPENAI_KEY_FOR_CHATVISION}"
      },
      "openAiEndpoint": {
        "value": "${AZURE_OPENAI_ENDPOINT}"
      },
      "createRoleForUser": {
        "value": "${CREATE_ROLE_FOR_USER=true}"
      },
      "acaExists": {
        "value": "${SERVICE_ACA_RESOURCE_EXISTS=false}"
      },
      "disableKeyBasedAuth": {
        "value": "${DISABLE_KEY_BASED_AUTH=true}"
      }
    }
  }
